{
  "backend": {
    "port": 8088,
    "db_path": "./kms.db",
    "host": "localhost"
  },
  "frontend": {
    "port": 8888,
    "api_url": "http://localhost:8088"
  },
  "database": {
    "path": "./kms.db",
    "type": "boltdb"
  },
  "server": {
    "read_timeout": 15,
    "write_timeout": 15,
    "idle_timeout": 60
  },
  "cors": {
    "allowed_origins": [
      "http://localhost:*",
      "http://127.0.0.1:*",
      "http://ctxdev.atomiton.com:*",
      "https://ctxdev.atomiton.com:*"
    ],
    "allow_all_origins": false
  }
}

